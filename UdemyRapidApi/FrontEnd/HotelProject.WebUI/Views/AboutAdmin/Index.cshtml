@model List<AboutResultDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}




<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Rezervasyonlar</h4>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered zero-configuration">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Birinci Başlık</th>
                                    <th>İkinci Başlık</th>
                                   
                                    <th>İçerik</th>
                                    <th>Oda Sayısı</th>
                                    <th>Çalın Sayısı</th>
                                    <th>Toplam Müşteri</th>
                                    <th>İşlemler</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.AbouId</td>
                                        <td>@item.Title1</td>
                                        <td>@item.Title2</td>
                                        <td>@item.Content</td>
                                      
                                        <td>@item.RoomCount</td>
                                        <td>@item.StaffCount</td>
                                        <td>@item.CustomerCount</td>
                                        
                                        <td><a href="/AboutAdmin/Update/@item.AbouId" class="btn btn-primary">Onayla</a></td>
                                       


                                    </tr>
                                }

                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>#</th>
                                    <th>Birinci Başlık</th>
                                    <th>İkinci Başlık</th>
                                
                                    <th>İçerik</th>
                                    <th>Oda Sayısı</th>
                                    <th>Çalın Sayısı</th>
                                    <th>Toplam Müşteri</th>
                                    <th>İşlemler</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>